{
    "compilerOptions": {
      "target": "ES2017",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "typeRoots": ["./types", "./node_modules/@types"]
    },
    "include": ["src/utils/**/*"],
    "exclude": ["node_modules"]
  }